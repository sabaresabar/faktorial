package faktorial;

import java.util.Scanner;

/**
 *
 * @author PRAKTIKUM
 */
public class Faktorial {

    public static void main(String[] args) {
        int a, b, c;
        Scanner reader = new Scanner(System.in);
        
        System.out.print("Masukkan angka : ");
        a = reader.nextInt();
        c=a;
        
        for (int x = 1; x <= a; x++) {
            b=c*c;
            c--;
            
            System.out.print(c);
        }
    }
    
}
